{
  "extends": [
    "eslint-config-airbnb"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "eslint:recommended",
    "@typscript-eslint",
    "plugin:import/recommended",
    "plugin:import/typescript"
  ],
  "settings": {
    "typescript": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error"
  }
}
